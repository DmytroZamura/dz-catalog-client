  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng run uafine:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "server": "node local.js",
    "build:browser:serverless": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:serverless": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng run uafine:server:serverless && webpack --config webpack.server.config.js --progress --colors",
    "dev:ssr": "ng run uafine:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod --build-optimizer && ng run uafine:server:production",
    "prerender": "ng run uafine:prerender",
    "serve:sls": "serverless offline start",
    "build:sls": "ng build --prod --base-href /test/ --build-optimizer && ng run uafine:serverless:production",
    "postinstall": "ngcc"
  },
